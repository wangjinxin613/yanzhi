.box {
	width: 100%;
	padding-left: 10%;
	padding-right: 10%;
	position: relative;
	height:690px;
}

#title { 
   
    margin-left:50%;
	width: 50%;
	height: 44px;
	line-height: 48px;
	border-left: 20px solid transparent;
	border-bottom: 44px solid #EFEFEF;
	 font-family:'华文行楷';
	
	margin-top: 52px;
	font-size: 30px;
	
	color:	#808080!important; text-align:right; padding-right:10px; 
	
}

.logo {filter:alpha(opacity=60);
	opacity:0.6;
	height: 45px;
	position: absolute;
	left: 100px;
	top: 0;
	
}

#samllTitle {
	font-family:'黑体';
	font-size: 26px;
	font-weight:900;
	color: #000;
}

#summary {
	color: #333;
	position: absolute;
	bottom: 20px;
	width:840px;
	
}
#line {
	width: 895px;
	border-bottom: 1px #03A9f4 solid;
	position: absolute;
	bottom: 50px;
	height: 1px;
}
.blank {
	width: 100%;
	height: 1px;
	border: 1px #489BD6 solid;
	margin-bottom: 12px;
}
 table td{ padding:5px!important; font-size:12px; text-align:center;  }
table th{ padding:5px!important;font-size:13px; text-align:center; }
.page {
	position: absolute;
	bottom: -20px;
	right: 8%;
	width: 35px;
	height: 35px;
	background-color: dodgerblue!important;
	border-radius: 50%;
}

.page span {
	height: 35px;
	line-height: 35px;
	display: block;
	color: #FFF!important;
	text-align: center
}
